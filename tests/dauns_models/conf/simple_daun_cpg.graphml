<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
			http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

   <!-- Attributes for nodes -->
   <key id="neuron_name" for="node" attr.name="name" attr.type="string">
     <default>"neuron"</default>
   </key>
   
   <key id="neuron_type" for="node" attr.name="type" attr.type="string">
     <default>lif_daun_interneuron</default>
   </key>

   <key id="pos_x" for="node" attr.name="x" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="pos_y" for="node" attr.name="y" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="color" for="node" attr.name="color" attr.type="string">
     <default>red</default>
   </key>
   
  <!-- Attributes for edges -->
   <key id="weight" for="edge" attr.name="weight" attr.type="double">
     <default>0.0</default>
   </key>

   <key id="g_syn" for="edge" attr.name="g_syn" attr.type="double">
     <default>0.0</default>
   </key>

   <key id="e_syn" for="edge" attr.name="e_syn" attr.type="double">
     <default>0.0</default>
   </key>

   <key id="v_h_s" for="edge" attr.name="v_h_s" attr.type="double">
     <default>0.0</default>
   </key>

   <key id="gamma_s" for="edge" attr.name="gamma_s" attr.type="double">
     <default>0.0</default>
   </key>

  <graph id="CPG" edgedefault="directed">

   <!-- CPG -->
       <node id="C1">
	 <data key="neuron_type">lif_daun_interneuron</data>
	 <data key="pos_x">1.0</data>
	 <data key="pos_y">0.0</data>
	 <data key="color">b</data>
       </node>

       <node id="C2">
	 <data key="neuron_type">lif_daun_interneuron</data>
	 <data key="pos_x">2.0</data>
	 <data key="pos_y">0.0</data>
	 <data key="color">g</data>
       </node>

       <node id="C3">
	 <data key="neuron_type">lif_daun_interneuron</data>
	 <data key="pos_x">4.0</data>
	 <data key="pos_y">0.0</data>
	 <data key="color">b</data>
       </node>

       <node id="C4">
	 <data key="neuron_type">lif_daun_interneuron</data>
	 <data key="pos_x">5.0</data>
	 <data key="pos_y">0.0</data>
	 <data key="color">g</data>
       </node>
       

   <!-- EDGES -->     
   <edge source="C1" target="C2">
     <data key="weight">0.0</data>
     <data key="g_syn">1.0</data>
     <data key="e_syn">-80.0</data>
     <data key="v_h_s">-43.0</data>
     <data key="gamma_s">-10.0</data>
   </edge>

   <edge source="C2" target="C1">
     <data key="weight">0.0</data>
     <data key="g_syn">1.0</data>
     <data key="e_syn">-80.0</data>
     <data key="v_h_s">-43.0</data>
     <data key="gamma_s">-10.0</data>
   </edge>

   <edge source="C3" target="C4">
     <data key="weight">0.0</data>
     <data key="g_syn">1.0</data>
     <data key="e_syn">-80.0</data>
     <data key="v_h_s">-43.0</data>
     <data key="gamma_s">-10.0</data>
   </edge>

   <edge source="C4" target="C3">
     <data key="weight">0.0</data>
     <data key="g_syn">1.0</data>
     <data key="e_syn">-80.0</data>
     <data key="v_h_s">-43.0</data>
     <data key="gamma_s">-10.0</data>
   </edge>
  </graph>
</graphml>
