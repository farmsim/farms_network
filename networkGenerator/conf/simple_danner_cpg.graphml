<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
			http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">

   <!-- Attributes for nodes -->
   <key id="neuron_name" for="node" attr.name="name" attr.type="string">
     <default>"neuron"</default>
   </key>
   
   <key id="neuron_type" for="node" attr.name="type" attr.type="string">
     <default>lif_danner_nap</default>
   </key>

   <key id="ext_in" for="node" attr.name="is_ext" attr.type="boolean">
     <default>true</default>
   </key>

   <key id="pos_x" for="node" attr.name="x" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="pos_y" for="node" attr.name="y" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="color" for="node" attr.name="color" attr.type="string">
     <default>red</default>
   </key>

   <key id="m_e" for="node" attr.name="m_e" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="m_i" for="node" attr.name="m_i" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="b_e" for="node" attr.name="b_e" attr.type="float">
     <default>0.0</default>
   </key>

   <key id="b_i" for="node" attr.name="b_i" attr.type="float">
     <default>0.0</default>
   </key>
   
  <!-- Attributes for edges -->
   <key id="weight" for="edge" attr.name="weight" attr.type="double">
     <default>0.0</default>
   </key>

  <graph id="CPG" edgedefault="directed">

   <!-- CPG -->
       <node id="RG_F">
	 <data key="ext_in">false</data>
	 <data key="neuron_type">lif_danner_nap</data>
	 <data key="pos_x">1.0</data>
	 <data key="pos_y">0.0</data>
	 <data key="color">b</data>
	 <data key="m_e">0.1</data>
	 <data key="b_e">0.0</data>	 
       </node>

       <node id="RG_E">
	 <data key="ext_in">false</data>
	 <data key="neuron_type">lif_danner_nap</data>
	 <data key="pos_x">1.0</data>
	 <data key="pos_y">4.0</data>
	 <data key="color">b</data>
	 <data key="m_e">0.0</data>
	 <data key="b_e">0.1</data>	 	 
       </node>

       <node id="In_F">
	 <data key="ext_in">false</data>
	 <data key="neuron_type">lif_danner</data>
	 <data key="pos_x">0.0</data>
	 <data key="pos_y">2.0</data>
	 <data key="color">g</data>	 
       </node>

       <node id="In_E">
	 <data key="ext_in">false</data>
	 <data key="neuron_type">lif_danner</data>
	 <data key="pos_x">2.0</data>
	 <data key="pos_y">2.0</data>
	 <data key="color">g</data>	 	 
       </node>


   <!-- EDGES -->     
       <edge source="RG_F" target="In_F">
	 <data key="weight">0.4</data>
       </edge>
       <edge source="In_F" target="RG_E">
	 <data key="weight">-1</data>
       </edge>
       <edge source="RG_E" target="In_E">
	 <data key="weight">0.4</data>
       </edge>
       <edge source="In_E" target="RG_F">
	 <data key="weight">-0.08</data>
       </edge>
  </graph>
</graphml>
