<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns
			http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
  <!-- Attributes for nodes -->
   <key id="neuron_name" for="node" attr.name="name" attr.type="string">
    <default>"neuron"</default>
   </key>

   <key id="neuron_type" for="node" attr.name="type" attr.type="string">
    <default>"interneuron"</default>

   </key>
      <key id="neuron_model" for="node" attr.name="neuron">
  </key>
  <!-- Attributes for edges -->
  <key id="synapse_weight" for="edge" attr.name="synapse">
  </key>
     <graph id="CPG" edgedefault="directed">
       <node id="n0">
	 <data key="neuron_name">neuron1</data>
	 <data key="neuron_type">interneuron</data>
       </node>
       <node id="n1" />
       <node id="n2" />
       <node id="n3" />

       <edge source="n0" target="n1" />
       <edge source="n0" target="n0" />
       <edge source="n1" target="n2" />
       <edge source="n2" target="n3" />
     </graph>
</graphml>
